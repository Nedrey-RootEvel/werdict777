#include <iostream>
#include <ctime>

using namespace std;

int main()
{
	setlocale(LC_ALL, "rus");
	srand(time(NULL));
	
	int const size1 = 10, size2 = 10;

	int arr1[size1][size2];
	
  cout << "And result:" << endl;
	for (int i1 = 0;i1 < size1;++i1)
	{
		for(int i2=0;i2<size2;++i2)
		arr1[i1][i2] = rand() % 90+10;
	}
	for (int i1 = 0; i1 < size1;++i1)
	{
		
		for (int i2 = 0;i2 < size2;++i2)
		cout << arr1[i1][i2];
		cout << endl;
	}

cout << "Speaker (- \a";
	return 0;
}


